{
	"name": "@vafast/cors",
	"version": "0.1.2",
	"description": "CORS middleware plugin for Vafast framework",
	"type": "module",
	"author": {
		"name": "Gaurish Sethia",
		"url": "https://github.com/gaurishhs",
		"email": "gaurishh.sethia@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/gaurishhs/@vafast/cors"
	},
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"import": "./dist/index.mjs",
			"default": "./dist/index.mjs"
		}
	},
	"bugs": "https://github.com/gaurishhs/@vafast/cors/issues",
	"homepage": "https://github.com/gaurishhs/@vafast/cors",
	"keywords": [
		"@vafast",
		"typescript",
		"cors"
	],
	"scripts": {
		"build": "tsdown",
		"dev": "npx tsx watch example/index.ts",
		"test": "npx vitest run",
		"release": "npm run build && npm run test && npx bumpp && npm publish --access=public"
	},
	"license": "MIT",
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"devDependencies": {
		"@types/debug": "^4.1.12",
		"@types/node": "^24.3.0",
		"rimraf": "^6.0.1",
		"tsdown": "^0.19.0-beta.4",
		"typescript": "^5.4.5",
		"vitest": "^4.0.16"
	},
	"peerDependencies": {
		"vafast": "<2.0.0"
	}
