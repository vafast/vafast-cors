{
	"name": "@vafast/cors",
	"version": "0.0.3",
	"description": "CORS middleware plugin for Vafast framework",
	"type": "module",
	"author": {
		"name": "Gaurish Sethia",
		"url": "https://github.com/gaurishhs",
		"email": "gaurishh.sethia@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/gaurishhs/@vafast/cors"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"bugs": "https://github.com/gaurishhs/@vafast/cors/issues",
	"homepage": "https://github.com/gaurishhs/@vafast/cors",
	"keywords": [
		"@vafast",
		"typescript",
		"cors"
	],
	"scripts": {
		"build": "tsup",
		"dev": "npx tsx watch example/index.ts",
		"test": "npx vitest run",
		"release": "npm run build && npm run test && npx bumpp && npm publish --access=public"
	},
	"license": "MIT",
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"devDependencies": {
		"@types/debug": "^4.1.12",
		"@types/node": "^24.3.0",
		"rimraf": "^6.0.1",
		"tsup": "^8.5.1",
		"typescript": "^5.4.5",
		"vafast": "^0.3.0",
		"vitest": "^4.0.16"
	},
	"peerDependencies": {
		"vafast": "^0.3.0"
	}
}
